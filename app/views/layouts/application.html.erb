<!DOCTYPE html>
<html>
<head>
  <title>My Recepies</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <%= link_to 'myRecepies', "", class: "btn btn-lg btn-success", id: "logo" %>
        <div class="nav-item">
          <%= link_to 'link1', "", class: "btn btn-default btn-warning"  %>
        </div>
        <div class="nav-item">
          <%= link_to 'link2', "", class: "btn btn-default btn-warning"  %>
        </div>
        <div class="nav-item">
          <%= link_to 'link3', "", class: "btn btn-default btn-warning"  %>
        </div>
      </div>
  </header>
  
  <div class="container">
    <%= yield %>
    <div class="footer">
      <p>&copy; MyRecepies 2016</p>
    </div>
  </div>

</body>
</html>
